<div id="<%= dom_id company %>">
  <p>
    <strong>ID:</strong>
    <%= company.id %>
  </p>
  
  <p>
    <strong>Company name:</strong>
    <%= company.company_name %>
  </p>

  <p>
    <strong>Cnpj:</strong>
    <%= company.cnpj %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= company.phone %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= company.address %>
  </p>

  <p>
    <strong>Address number:</strong>
    <%= company.address_number %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= company.country %>
  </p>

  <p>
    <strong>State:</strong>
    <%= company.state %>
  </p>

  <p>
    <strong>City:</strong>
    <%= company.city %>
  </p>

</div>
