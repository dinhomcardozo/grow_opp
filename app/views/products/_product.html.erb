<div id="<%= dom_id product %>">
  <p>
    <strong>Type:</strong>
    <%= product.product_type %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= product.status %>
  </p>

  <p>
    <strong>License:</strong>
    <%= product.license %>
  </p>

</div>
